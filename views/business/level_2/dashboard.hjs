    <script>
        var userid = "{{eId}}";
        var username = "{{employeeName}}";
        var userlevel = "{{employeePermission}}";
        var company = "{{companyName}}";

        if (userid == "") {
            userid = "newID";
        }
        if (username == "") {
            username = "newName";
        }
        if (userlevel == "") {
            userlevel = "0";
        }
        if (company == "") {
            company = "newCompany";
        }

    </script>

</head>
<body>

  <div class="page-wrap">
    <!-- nav bar -->
    <div class = "top-bar-section">
      <div style="padding-top: 33px;">
        <span class="shameless-plug">Herald</span>
      </div>
      <div class="form-container">
        {{#message}}
          <div style="color: #FFFFFF">{{message}}</div>
        {{/message}}
      </div>
      <ul class="nav navbar-nav navbar-right navbar-padding nav-links">
        <li class="active">
          <a href="/dashboard">Dashboard</a>
        </li>
        <li id="emptab">
            <a href="/addemployees">Employees</a>
          </li>
        <li>
          <a href="/accountSettings">Account Settings</a>
        </li>
        <li>
          <a href="/businesssetting">Company Settings</a>
        </li>

        <li>
          <a href="/logout">Logout</a>
        </li>
      </ul>


    </div>

  </nav>
  <div class="container container-margin-no-secondary">


  </div>

<div class="container transbox" style="box-shadow:  0px 0px 30px 15px rgba(0, 0, 0, 0.8)">


{{#message}}
   <div class="alert alert-danger">{{message}}</div>
{{/message}}

  <h1><strong><div>Hi, {{companyName}}  ({{employeePermission}})</div></strong></h1>
  <div class="dashboardTopInfo text-center">

      <h1 class="text-center">Today</h1>
      <h1 class="text-center" id="currentDate" ></h1>
      <div id="txt" class="text-center"></div>
    </div>

      <button id="addevent" onclick="recordClick(userid, username, userlevel, company)" style="color:black">Add Event</button>
    <p> Add event: simply records the user and permission level and current time </p>
      <button id="retrieveevent" onclick="testkeenio()" style="color:black">Retrieve Event</button>
    <p>Retrieve events: simply calls the function that creates the charts below. (Not much meaningful data exists so expect them to look plain/empty)</p>



    </div> <!-- end transbox top -->


  <div class="container transbox" style="box-shadow: 0px 0px 30px 15px rgba(0, 0, 0, 0.8)">


      <div class="dashboardTopInfo text-center">
          <h1><strong><div>Analytics</div></strong></h1>
          <h1 class="text-center" id="currentDate" ></h1>
          <div id="txt" class="text-center"></div>
      </div>

      <div id="chart_wrapper">

          <div class="container-fluid">
              <div class="row">

                  <div class="col-sm-6">
                      <div class="chart-wrapper">
                          <div class="chart-title">
                              Pageviews by browser (past 24 hours)
                          </div>
                          <div class="chart-stage" id="chart-01">


                          </div>
                          <div class="chart-notes">
                              This is a sample text region to describe this chart.
                          </div>
                      </div>
                  </div>

                  <div class="col-sm-6">
                      <div class="chart-wrapper">
                          <div class="chart-title">
                              Pageviews by browser (past 5 days)
                          </div>
                          <div class="chart-stage" id="chart-02">

                          </div>
                          <div class="chart-notes">
                              Notes go down here
                          </div>
                      </div>
                  </div>

              </div>


              <div class="row">

                  <div class="col-sm-12">
                      <div class="chart-wrapper">
                          <div class="chart-title">
                              Impressions by advertiser
                          </div>
                          <div class="chart-stage" id="chart-03">

                          </div>
                          <div class="chart-notes">
                              Notes go down here
                          </div>
                      </div>
                  </div>

              </div>

      </div> <!-- end chart -->

  </div> <!-- end transbox bottom -->



  </div> <!-- end page -->


